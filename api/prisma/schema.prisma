generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model doadores {
  id         Int      @id @default(autoincrement())
  nome       String
  email      String   @unique
  nascimento DateTime
  infoiapes  Boolean  @default(false)
  createdAt  DateTime @default(now())
}
